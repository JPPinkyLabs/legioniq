{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "^/$",
      "status": 308,
      "headers": { "Location": "/mvp1/" }
    },
    {
      "src": "/mvp1/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/mvp1$",
      "dest": "/index.html"
    },
    {
      "src": "/mvp1/(.*)",
      "dest": "/index.html"
    }
  ]
}

